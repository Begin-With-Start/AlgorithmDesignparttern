<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="designpartern.minifly.com.designparttern.algorithm.SortingMergeActivity">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        >

       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:orientation="vertical"
           >

           <TextView
               android:id="@+id/algorithm_content_fbnq_id"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text=""
               app:layout_constraintLeft_toLeftOf="parent"
               />

           <TextView
               android:id="@+id/algorithm_compute_fbnq_id"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginTop="10dp"
               app:layout_constraintTop_toBottomOf="@+id/algorithm_content_id"
               />

       </LinearLayout>

    </ScrollView>

</android.support.constraint.ConstraintLayout>
